{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Kickz Investments",
  "logo": {
    "dark": "/logo/MM_logo_blue.png",
    "light": "/logo/MM_logo_white.png"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "topbarLinks": [
    {
      "name": "Gitbook",
      "url": "https://app.gitbook.com/"
    },
    {
      "name": "Projects",
      "url": "/gitbook/projects"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://dashboard.mintlify.com"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    },
    {
      "name": "Tools",
      "url": "gitbook"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://mintlify.com/docs"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "introduction",
        "quickstart",
        "development"
      ]
    },
    {
      "group": "Tools",
      "pages": [
        "gitbook/tools/index",
        "gitbook/tools/apexcharts",
        "gitbook/tools/libraries"
      ]
    },
    {
      "group": "Gitbook",
      "pages": [
        "gitbook/Mintlify",
        "gitbook/docker",
        "gitbook/dashboard",
        "gitbook/gitbook-note",
        "gitbook/annual-report",
        "gitbook/projects"
      ]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": [
        "api-reference/introduction"
      ]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/mintlify"
  },
  "headLinks": [
    {
      "rel": "stylesheet",
      "href": "https://cdn.jsdelivr.net/npm/daisyui@4.12.23/dist/full.min.css"
    }
  ],
  "headScripts": [
    {
      "src": "https://cdn.tailwindcss.com",
      "async": true
    },
    {
      "src": "https://cdn.jsdelivr.net/npm/apexcharts",
      "async": true
    }
  ]
}